<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="Page">

	<resultMap class="java.util.HashMap" id="pageMap">
		<result property="pageid" column="pageid" />
		<result property="pagename" column="pagename" />
	</resultMap>

	<select id="getPage" parameterClass="int" resultClass="String">
		select pagename from page
		where pageid = #pageId#
	</select>
	
</sqlMap>